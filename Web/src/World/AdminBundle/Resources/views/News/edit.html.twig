{% extends 'WorldDashboardBundle::admin.html.twig' %}

{% trans_default_domain 'WorldDashboardBundle' %}

{% form_theme edit_form 'WorldDashboardBundle::themes/theme.html.twig'  %}
{% form_theme delete_form 'WorldDashboardBundle::themes/delete_theme.html.twig'  %}

{% block breadcrumb %}{% endblock %}

{% block body -%}

    {% embed "WorldToolBundle:Base:title.html.twig" with { 'title': entity.routeName ~ '.edit' } %} {% endembed %}

    {{ form(edit_form) }}

    {% embed "WorldToolBundle:Base:edit.back.html.twig" with { 'entity': entity, 'delete_form': delete_form } %} {% endembed %}

{% endblock %}
