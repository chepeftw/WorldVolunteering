body{font-family:Arial, Helvetica, sans-serif;font-size:12px;margin:0;padding:0}h1,h2,h3{font-weight:400;color:#023848;margin:0;padding:0}h1{font-size:2em}h2{font-size:2.4em}h3{font-size:1.6em}p,ul,ol{margin-top:0;line-height:150%}a{text-decoration:none;color:#4486C7}#wrapper{width:980px;background-color:#FFF;margin:0 auto;padding:0}#header{width:980px;height:260px;padding-top:5px;background:url(images/img06_2.gif) no-repeat left top;margin:0 auto}#logo{float:right;color:#000;width:350px;margin:0;padding:0}#logo h1{float:left;letter-spacing:-1px;text-align:right;font-size:3.8em;border:none;background:none;color:#FFF;margin:0;padding:200px 20px 0 0}#logo_up{float:left;color:#FFF;width:300px;margin:0;padding:0}#logo_up h1{float:left;padding-top:15px;padding-left:20px;font-size:2.6em;border:none;background:none;color:#FFF}#search{float:right;width:280px;height:60px;background:#E2E2E2;border-bottom:4px solid #FFF;padding:20px 0 0}#search form{height:41px;margin:0;padding:10px 0 0 20px}#search fieldset{border:none;margin:0;padding:0}#search-text{width:170px;border:1px solid #DEDEDE;background:#FFF;text-transform:lowercase;font:normal 11px Arial, Helvetica, sans-serif;color:#5D781D;padding:6px 5px 2px}#search-submit{width:50px;height:22px;border:none;background:#B9B9B9;color:#000}#menu{width:980px;height:50px;margin:0 auto;padding:0}#menu ul{list-style:none;line-height:normal;margin:0;padding:15px 0 0}#menu a{display:block;width:130px;height:30px;margin-right:1px;background:url(images/img02.gif) no-repeat left top;text-decoration:none;text-align:center;font-family:Arial, Helvetica, sans-serif;font-size:13px;font-weight:700;color:#FFF;border:none;padding:9px 0 0}#menu a:hover,#menu .current_page_item a{text-decoration:none}#menu .current_page_item a{padding-left:0}#page{width:940px;margin:0 auto;padding:10px 0 0 20px}#page-bgtop{padding:10px 0}#content{float:left;width:720px;padding:0}.post{background:url(images/img07.gif) no-repeat left 50px;margin-bottom:15px}.post .title{height:38px;margin-bottom:10px;letter-spacing:-.5px;color:#000;padding:12px 0 0}.post .title a{color:#023848;border:none}.post .meta{margin-bottom:30px;text-align:left;font-family:Arial, Helvetica, sans-serif;font-size:13px;font-weight:700;padding:5px 0 15px}.post .meta .posted{float:right}.post .entry{background:url(images/img08.gif) no-repeat left top;text-align:justify;padding:0 0 20px}.links{padding-top:20px;font-size:12px;font-weight:700}#sidebar{float:right;width:280px;color:#787878;border-left:1px #787878 solid;margin:0;padding:0 5px 0 15px}#sidebar ul{list-style:none;margin:0;padding:0}#sidebar li{margin:0;padding:0}#sidebar li ul{padding-bottom:30px;margin:0}#sidebar li li{line-height:35px;border-bottom:1px dashed #D1D1D1;border-left:none;margin:0 30px}#sidebar li li span{display:block;margin-top:-20px;font-size:11px;font-style:italic;padding:0}#sidebar h2{height:38px;padding-left:30px;letter-spacing:-.5px;font-size:1.8em}#sidebar p{text-align:justify;margin:0;padding:0 30px 20px}#sidebar a{border:none}#sidebar a:hover{text-decoration:underline;color:#8A8A8A}#calendar_wrap{padding:20px}#calendar table{width:100%}#calendar tbody td{text-align:center}#calendar #next{text-align:right}#footer{height:35px;background:#ECECEC;border-top:1px solid #DEDEDE;font-family:Arial, Helvetica, sans-serif;margin:0 auto;padding:0 0 10px}#footer p{padding-top:20px;line-height:normal;font-size:9px;text-transform:uppercase;text-align:center;color:#A0A0A0;margin:0}#footer a{color:#8A8A8A}.innerblock-content{margin-left:7px;margin-right:7px;text-align:left}.innerblock-content h4{color:#555;font-size:120%;margin-bottom:2px;margin-left:3px;margin-top:2px}.pic{display:block;float:left;margin-top:5px;text-align:center;width:100px}.info{display:block;float:left;margin-left:8px;padding-top:5px;width:370px;text-align:justify}.info_assoc{display:block;float:left;margin-left:8px;padding-top:5px;width:480px;text-align:justify}.info_assoc_news{display:block;float:left;margin-left:8px;padding-top:5px;width:auto;text-align:justify}.info_show{display:block;float:left;margin-left:8px;padding-top:10px;width:600px;text-align:justify}.info_show_activity{display:block;float:left;margin-left:10px;padding-top:10px;width:450px;text-align:justify}.functions{display:block;float:left;margin-left:20px;margin-top:10px;width:100px}.functions-show{display:block;float:left;margin-left:25px;margin-top:10px;width:auto}.cleanse{clear:both}#new-link{font-size:18px;color:#006;text-decoration:none;margin:10px}#img-pagination{background:#fff;border:0;padding:0}.pagination{text-align:right;padding-right:30px}form th{padding-right:10px;text-align:right}ul.error_list{border:solid 1px red;list-style-type:none;margin-top:15px;margin-bottom:5px}ul.error_list li{color:red}p#alert{font-size:22px;line-height:1.6em;font-style:italic;letter-spacing:-.5px;color:#7ba45b;width:auto;border-bottom:1px solid #ffd900;border-top:1px solid #ffd900;padding-left:30px}#flash_notice{margin-left:auto;margin-right:auto}.error_div{background-color:red;color:#FFF;margin:2px}table th{width:180px}legend,label{font-weight:700}#save{width:720px;text-align:center}ul{list-style-type:none;padding:0}#menu li,.post .meta .date{float:left}
